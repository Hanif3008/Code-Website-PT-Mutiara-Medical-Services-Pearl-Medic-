                <td><%= Medical.nomorpesanan %></td>
                <td><%= new Date(Medical.tanggalpemesanan_medical).toLocaleDateString('id-ID') %></td>
                <td><%= Medical.jumlahpasien_medical %></td>
                <td id="reds">Belum Dilayani</td>
                <td><a href="/LPelayananDetailMedical_admin/<%= Medical.id_medical %>">Detail</a></td>
              </tr>
              <% hasData = true; %> <% } %> <% }); %> <% if (!hasData) { %>
